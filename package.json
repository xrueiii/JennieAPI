{
  "name": "jennieapi",
  "displayName": "JennieAPI",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jennieapi.helloWorld",
    "onCommand:jennieapi.testapiCmd",
    "onCommand:jennieapi.openWebview",
    "onCommand:jennieapi.insertApiCode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jennieapi.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "jennieapi.testapiCmd",
        "title": "Test API"
      },
      {
        "command": "jennieapi.openWebview",
        "title": "JennieAPI: Open API Connector"
      },
      {
        "command": "jennieapi.insertApiCode",
        "title": "JennieAPI: 插入 API 程式碼"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "jennieapi.insertApiCode",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "node-fetch": "^3.3.2"
  }
}
